<div class="container">
  <div class="container-inner inner-sm clearfix">
    <% @have_ranking_items.each do |item| %>
    <div class="item-box">
      <div class="image">
        <div class="img-wrap">
            <% if image_tag item.large_image %>
              <%= image_tag(item.large_image , alt: item.title) %>
            <% else %>
              <%= image_tag("no-image.png" , alt: item.title) %>
            <% end %>
        </div>
      </div>
      <div class="detail">
        <p class="title"><%= item.title %></p>
      </div>
    </div>
    <% end %>
  </div>
</div>